{
	"workbench.colorTheme": "Markdown Editor Dark",
	"editor.minimap.enabled": false,
	"breadcrumbs.enabled": false,
	"editor.glyphMargin": false,
	"explorer.decorations.badges": false,
	"explorer.decorations.colors": false,
	"editor.fontLigatures": true,
	"git.enableSmartCommit": true,
	"window.commandCenter": true,
	"editor.renderWhitespace": "none",
	"markdown.validate.enabled": true,
	"markdown.updateLinksOnFileMove.enabled": "prompt",
	"workbench.startupEditor": "none",
	"workbench.editor.empty.hint": "hidden",
	"editor.fontFamily": "'NotosansM Nerd Font', 'monospace', monospace",
	"editor.fontSize": 16,
	"markdown.preview.fontSize": 16,
	"terminal.integrated.fontFamily": "'IosevkaTerm Nerd Font Mono', 'monospace'",
	"terminal.integrated.fontSize": 16,
	"markdown.preview.fontFamily": "NotosansM Nerd Font, -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
	"files.autoSave": "afterDelay",
}
